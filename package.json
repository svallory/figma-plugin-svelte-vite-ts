{
  "name": "figma-plugin-template",
  "displayName": "Figma Plugin Template",
  "description": "A template for creating Figma plugins in Typescript using Svelte and Vite",
  "homepage": "https://github.com/tokilabs/figma-plugin-template",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Saulo Vallory",
    "email": "saulo@toki.life"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "svelte": "^3.54.0",
    "svelte-check": "^2.10.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-singlefile": "^0.13.2"
  },
  "keywords": [
    "figma",
    "plugin",
    "svelte",
    "vite",
    "typescript"
  ],
  "files": [
    "plugin"
  ],
  "main": "./plugin/core.umd.cjs",
  "module": "./plugin/core.js",
  "exports": {
    ".": {
      "import": "./plugin/core.js",
      "require": "./plugin/core.umd.cjs"
    }
  },
  "contributors": [
    {
      "name": "Saulo Vallory",
      "email": "saulo@toki.life"
    }
  ]
}
